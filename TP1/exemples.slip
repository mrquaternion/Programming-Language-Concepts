;;; Quelques exemples.                  -*- mode: lisp-data; coding: utf-8 -*-

2                                       ; ↝ 2
+                                       ; ↝ <primitive>
(+ 2 4)                                 ; ↝ 6

((fob (x) x) 2)                         ; ↝ 2

(((fob (x) (fob (y) (* x y)))
  3)
 5)                                     ; ↝ 15

(fix (((even x)  (if (= x 0) true  (odd  (- x 1))))
      ((odd x)   (if (= x 0) false (even (- x 1)))))
  (odd 42))                             ; ↝ False
